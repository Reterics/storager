{
  "name": "storager",
  "displayName": "StorageR",
  "description": "Manage your store in an easy way",
  "private": true,
  "version": "1.1.6",
  "repository": {
    "url": "https://github.com/Reterics/storager",
    "type": "git"
  },
  "author": {
    "email": "reterics.attila@gmail.com",
    "url": "https://reterics.com",
    "name": "Attila Reterics"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --mode dev",
    "build:test": "tsc -b && vite build --mode test",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage --no-cache",
    "e2e:codegen": "npx playwright codegen",
    "e2e:test": "npx playwright test"
  },
  "dependencies": {
    "@playwright/test": "^1.51.0",
    "@react-pdf/render": "4.3.0",
    "@react-pdf/renderer": "4.3.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "firebase": "^11.4.0",
    "html2canvas": "^1.4.1",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.0.4",
    "jspdf": "^3.0.1",
    "leaflet": "^1.9.4",
    "prettier": "^3.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "react-leaflet": "5.0.0",
    "react-router-dom": "^6.27.0",
    "react-signature-pad-wrapper": "4.1.0",
    "recharts": "^2.15.2",
    "throttle-debounce": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@react-pdf/types": "2.7.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@types/leaflet": "^1.9.12",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/throttle-debounce": "^5.0.2",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "3.0.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fake-indexeddb": "^6.0.0",
    "globals": "^15.11.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "rollup": "^4.35.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.26.1",
    "vite": "6.2.6",
    "vitest": "^3.0.8",
    "vitest-monocart-coverage": "3.0.1"
  }
}
