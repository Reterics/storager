{
  "name": "storager",
  "displayName": "StorageR",
  "description": "Manage your store in an easy way",
  "private": true,
  "version": "1.1.7",
  "repository": {
    "url": "https://github.com/Reterics/storager",
    "type": "git"
  },
  "author": {
    "email": "reterics.attila@gmail.com",
    "url": "https://reterics.com",
    "name": "Attila Reterics"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --mode dev",
    "build:test": "tsc -b && vite build --mode test",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "preview": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage --no-cache",
    "e2e:codegen": "npx playwright codegen",
    "e2e:test": "npx playwright test",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-pdf/render": "4.3.0",
    "@react-pdf/renderer": "4.3.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "firebase": "^11.10.0",
    "html2canvas": "^1.4.1",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.2.0",
    "jspdf": "^3.0.1",
    "leaflet": "^1.9.4",
    "prettier": "^3.6.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-i18next": "^15.6.1",
    "react-icons": "^5.5.0",
    "react-leaflet": "5.0.0",
    "react-router-dom": "^6.30.1",
    "react-signature-pad-wrapper": "4.1.1",
    "recharts": "^2.15.4",
    "throttle-debounce": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@eslint/js": "^9.33.0",
    "@react-pdf/types": "2.9.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.6.4",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/throttle-debounce": "^5.0.2",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import-x": "^4.4.2",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unicorn": "^56.0.0",
    "fake-indexeddb": "^6.1.0",
    "globals": "^15.15.0",
    "husky": "^9.1.6",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.5.6",
    "rollup": "^4.46.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "6.3.5",
    "vitest": "^3.2.4",
    "vitest-monocart-coverage": "3.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  }
}
